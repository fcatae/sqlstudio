{
  "name": "sqlstudio",
  "displayName": "sqlstudio",
  "description": "SQL Code Editor",
  "version": "0.0.1",
  "publisher": "fcatae",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.execSql",
        "title": "ExecSQL"
      }
    ],
    "keybindings": [
      {
        "command": "extension.execSql",
        "key": "ctrl+E"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "tedious": "^1.14.0"
  }
}
